{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-console": "off",
    "max-len": ["error", { "code": 120 }],
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "func-names": "off"
  },
  "env": {
    "jest": true
  }
}
